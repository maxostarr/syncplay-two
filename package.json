{
  "name": "syncplay-two",
  "version": "0.1.0",
  "private": true,
  "author": "Max Starr",
  "email": "info@maxstarr.xyz",
  "description": "Replacement for Syncplay",
  "dependencies": {
    "@material-ui/core": "^4.0.0",
    "@material-ui/icons": "^4.0.0",
    "peerjs": "^1.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-player": "^1.11.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "set ELECTRON_START_URL=http://localhost:3000 && electron . --allow-file-access-from-files",
    "preelectron-pack": "yarn build",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/electron.js",
    "release": "release-it",
    "dev": "nf start"
  },
  "build": {
    "appId": "com.electron.syncplayTwo",
    "productName": "SyncplayTwo",
    "copyright": "Cpyright Â© 2020 Max Starr",
    "win": {
      "target": "portable"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "homepage": "./",
  "main": "src/electron-starter.js",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^5.0.2",
    "electron-builder": "^21.2.0"
  }
}
